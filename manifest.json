{
"name":"Fenaki",
"description":"Replaces remote HTML pages with local content",
"version":"1",
"manifest_version":2,
"browser_action" : {
  "default_popup": "fenaki.html"
},
"options_ui": {
    "page": "options.html",
    "chrome_style": true
},
"permissions": [
    "activeTab",
    "storage",
    "<all_urls>"
  ]
}